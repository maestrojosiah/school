<!--main content start-->
{% if app.request.attributes.get('_route') != 'login' %}    
    <section id="main-content">
    <section class="wrapper"> 
{% endif %}
             
    {% block title %}
        <h4>{{action_title}}</h4>
    {% endblock %}
    <!-- /.row -->
    {% block toplinks %}
        {{ include('templates/toplinks.html.twig') }}
    {% endblock %}

    {% block charts %}
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-8">
            {% block areachart %}
                {{ include('templates/areachart.html.twig') }}
            {% endblock %}

            {% block barchart %}
                {{ include('templates/barchart.html.twig') }}
            {% endblock %}

            {% block timeline %}
                {{ include('templates/timeline.html.twig') }}
            {% endblock %}

            {% block table %}
                {{ include('templates/table.html.twig') }}
            {% endblock %}

        </div>
        <!-- /.col-lg-8 -->
        <div class="col-lg-4">

            {% block notification %}
                {{ include('templates/notification.html.twig') }}
            {% endblock %}

            {% block donutchart %}
                {{ include('templates/donutchart.html.twig') }}
            {% endblock %}

            {% block chat %}
                {{ include('templates/chat.html.twig') }}
            {% endblock %}

        </div>
        <!-- /.col-lg-4 -->
    </div>
    <!-- /.row -->
    {% endblock %}
    <div class="row">
        <div class="col-lg-8">

            {% block otherstuff_lg8 %}
            {% endblock %}
        </div>
        <div class="col-lg-4">
            {% block otherstuff_lg4 %}
            {% endblock %}
        </div>
    </div>
    <div class="row">
        {% block otherstuff_lg12 %}
        {% endblock %}
    </div>
</div>
<!-- /#page-wrapper -->
{% if app.request.attributes.get('_route') != 'login' %}    
    </section> 
    </section>
{% endif %}
